// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "dist": true,
        "dist-bin": true,
        "**/*.js.map": true,
        "node_modules": true,
        "typings": true
    },
    "json.schemas": [
        {
            "fileMatch": [
                "/tsconfig.json"
            ],
            "url": "http://json.schemastore.org/tsconfig"
        }
    ]
}